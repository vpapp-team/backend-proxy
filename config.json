{
	"general": {
		"registration": {
			"hostname": "proxy.nigb.app",
			"port": 443,
			"method": "SUBSCRIBE",
			"path": "/login"
		},
		"broadcast": {
			"hostname": "broadcast.nigb.app",
			"port": 443,
			"method": "NOTIFY"
		},
		"httpsPorts": [443],
		"SECURE_CONTEXT": {
			"key": "/etc/letsencrypt/live/nigb.app/privkey.pem",
			"cert": "/etc/letsencrypt/live/nigb.app/cert.pem",
			"ca": "/etc/letsencrypt/live/nigb.app/fullchain.pem"
		},
		"publicKey": "./publicKey.pem"
	},
	"endpoints": [
		{
			"hostname": "@",
			"allowUnsecure": true
		},
		{
			"hostname": "admin.nigb.app",
			"allowUnsecure": false
		},
		{
			"hostname": "10.10.0.125",
			"allowUnsecure": true
		}
	]
}
